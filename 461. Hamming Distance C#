public class Solution {
    public int HammingDistance(int x, int y) {
        string a=Convert.ToString(x,2).PadLeft(32, '0');
        string b=Convert.ToString(y, 2).PadLeft(32, '0');
        Console.WriteLine(a+"\n"+b);
        int count=0;
        for (int i=0; i<a.Length; i++)
            if (a[i]!=b[i]) count++;
        return count;  
    }
}
